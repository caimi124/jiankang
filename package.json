{
  "name": "notion-herb-updater",
  "version": "1.0.3",
  "description": "自动化草药数据库Notion条目创建和更新工具",
  "main": "notion-herb-updater.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "echo 'Using app/sitemap.ts instead of next-sitemap'",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test-sitemap": "node test-sitemap.js",
    "test-analytics": "node test-google-analytics.js",
    "seo-check": "node scripts/seo-health-check.js",
    "notion": "node notion-herb-updater.js",
    "create-herbs": "node create-all-herbs.js",
    "manage-database": "node notion-database-manager.js",
    "notion:create-schema": "node scripts/create-notion-schema.js",
    "notion:seed-cinnamon": "node scripts/seed-cinnamon.js",
    "notion:seed-templates": "node scripts/seed-templates.js",
    "notion:migrate-csv": "node scripts/migrate-herbs-to-notion-csv.js scripts/notion-export-templates/generated/herbs.csv",
    "notion:add-relation": "node scripts/notion-add-herb-relation.js",
    "notion:link-orphans": "node scripts/notion-link-orphans.js --herb=cinnamon",
    "export-data": "node -e \"const {NotionDatabaseManager} = require('./notion-database-manager.js'); new NotionDatabaseManager().exportToCSV()\"",
    "stats": "node -e \"const {NotionDatabaseManager} = require('./notion-database-manager.js'); new NotionDatabaseManager().getStatsByCategory()\"",
    "test-website": "npm run build && npm run start",
    "check-seo": "node scripts/submit-to-google.js",
    "performance-audit": "npm run build && npm run check-seo",
    "sync-blog": "node scripts/sync-turmeric-blog-to-notion.js",
    "sync-blog-watch": "node scripts/real-time-blog-sync.js start",
    "sync-blog-status": "node scripts/real-time-blog-sync.js status",
    "sync-blog-manual": "node scripts/real-time-blog-sync.js sync",
    "sanity": "sanity dev",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "sanity:start": "sanity start",
    "sanity:upload-images": "node scripts/batch-upload-herb-images.js",
    "migrate:faqs": "node scripts/migrate-faqs-to-sanity.js",
    "migrate:dosages": "node scripts/migrate-dosages-to-sanity.js",
    "migrate:studies": "node scripts/migrate-studies-to-sanity.js",
    "migrate-herbs": "node scripts/migrate-herbs-to-sanity.js",
    "migrate-all": "node scripts/migrate-to-sanity.js",
    "seo:generate-pages": "node scripts/generate-herb-pages.js",
    "seo:generate-sitemap": "node scripts/generate-seo-optimized-sitemap.js",
    "seo:submit-priority": "node scripts/submit-to-google-search-console.js --priority",
    "seo:submit-all": "node scripts/submit-to-google-search-console.js --all",
    "seo:audit": "npm run build && npm run seo-check",
    "seo:full-setup": "npm run seo:generate-pages && npm run seo:generate-sitemap && npm run seo:submit-priority"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@notionhq/client": "^2.3.0",
    "@portabletext/react": "^3.2.1",
    "@sanity/client": "^7.11.2",
    "@sanity/color-input": "^4.0.5",
    "@sanity/image-url": "^1.1.0",
    "@sanity/ui": "^3.1.4",
    "@sanity/vision": "^4.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dotenv": "^16.6.1",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.516.0",
    "next": "^15.3.3",
    "next-sanity": "^10.0.3",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^19.1.1",
    "react-window": "^2.1.0",
    "sanity": "^4.1.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "zod": "^3.25.67"
  },
  "keywords": [
    "notion",
    "database",
    "herbs",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.3",
    "typescript": "^5.8.3"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caimi124/jiankang.git"
  },
  "bugs": {
    "url": "https://github.com/caimi124/jiankang/issues"
  },
  "homepage": "https://github.com/caimi124/jiankang#readme"
}
